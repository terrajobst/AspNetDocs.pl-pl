{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/aspnet-core-conceptual/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.pdf",
          "**/*.exe",
          "**/*.wmv",
          "**/*.zip"
        ],
        "exclude": [
          "**/obj/**",
          "aspnet/core/**",
          "**/includes/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "xrefService": [
      "https://xref.docs.microsoft.com/query?uid={uid}"
    ],
    "globalMetadata": {
      "_navPath": "/foo",
      "_navRel": "/foo",
      "breadcrumb_path": "/aspnet/breadcrumb/toc.json",
      "bilingual_type": "hover over",
      "ms.prod": "aspnet-framework",
      "ms.topic": "conceptual",
      "uhfHeaderId": "MSDocsHeader-DotNet",
      "searchScope": [
        "ASP.NET"
      ],
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.aspnet-conceptual",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {
      "ms.technology": {
        "ajax/**/**.md": "aspnet-ajax",
        "aspnet/**/**.md": "aspnet-aspnet",
        "identity/**/**.md": "aspnet-identity",
        "mobile/**/**.md": "aspnet-mobile",
        "mvc/**/**.md": "aspnet-mvc",
        "signalr/**/**.md": "aspnet-signalr",
        "single-page-application/**/**.md": "aspnet-spa",
        "visual-studio/**/**.md": "aspnet-visualstudio",
        "web-api/**/**.md": "aspnet-webapi",
        "web-forms/**/**.md": "aspnet-webforms",
        "web-pages/**/**.md": "aspnet-webpages",
        "webhooks/**/**.md": "aspnet-webhooks",
        "whitepapers/**/**.md": "aspnet-whitepapers"
      },
     "open_to_public_contributors": {
        "ajax/**.md": false,
        "aspnet/**.md": false,
        "identity/**.md": false,
        "mobile/**.md": false,
        "mvc/**.md": false,
        "signalr/**.md": false,
        "single-page-application/**.md": false,
        "visual-studio/**.md": false,
        "web-api/**.md": false,
        "web-forms/**.md": false,
        "web-pages/**.md": false,
        "webhooks/**.md": false,
        "whitepapers/**.md": false
      }
    },
    "template": [],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
